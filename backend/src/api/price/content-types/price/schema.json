{
  "kind": "collectionType",
  "collectionName": "prices",
  "info": {
    "singularName": "price",
    "pluralName": "prices",
    "displayName": "Price",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date",
      "required": true
    },
    "lowest": {
      "type": "decimal",
      "required": true
    },
    "highest": {
      "type": "decimal",
      "required": true
    },
    "unit": {
      "type": "enumeration",
      "enum": [
        "kg",
        "ton",
        "liter",
        "piece"
      ],
      "default": "kg"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "prices"
    },
    "city": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::city.city",
      "inversedBy": "prices"
    },
    "name": {
      "type": "string",
      "minLength": 5,
      "maxLength": 100
    },
    "email": {
      "type": "email"
    },
    "user_type": {
      "type": "enumeration",
      "enum": [
        "system",
        "producer",
        "merchant"
      ],
      "default": "system"
    },
    "ip": {
      "type": "string"
    }
  }
}
